function doorNumber(s){
	
	if(s == "Horse"){ return 0; }
	if(s == "Goat"){ return 1; }
	if(s == "Monkey"){ return 2; }
	if(s == "Rooster"){ return 3; }
	if(s == "Dog"){ return 4; }
	if(s == "Pig"){ return 5; }
	if(s == "Rat"){ return 6; }
	if(s == "Ox"){ return 7; }
	if(s == "Tiger"){ return 8; }
	if(s == "Rabbit"){ return 9; }
	if(s == "Dragon"){ return 10; }
	if(s == "Snake"){ return 11; }
	
	
	return -1;
}